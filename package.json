{
  "name": "pancake-profile-api",
  "version": "1.1.0",
  "description": "Serverless API implementation for PancakeSwap Profile smart contract on Binance Smart Chain.",
  "main": "api/version.ts",
  "repository": "git@github.com:pancakeswap/pancake-profile-api.git",
  "author": "PancakeSwap",
  "license": "MIT",
  "scripts": {
    "format:check": "prettier --check '*/**/*.{js,ts}'",
    "format:write": "prettier --write '*/**/*.{js,ts}'",
    "lint": "eslint '*/**/*.{js,ts}'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "ethereumjs-util": "^7.0.10",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "mongoose": "^5.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vercel/node": "^1.11.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^4.3.8",
    "jest": "^27.0.1",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
