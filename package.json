    "@typescript-eslint/parser": "^4.28.0",
    "@vercel/node": "^1.11.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^4.3.8",
